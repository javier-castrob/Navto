<mvc:View controllerName="EjemploNavTo.EjemploNavTo.controller.Main"  xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<Page id="page" title="{i18n>titleMain}">
		<content>
            <FlexBox
                alignItems="Start"
                justifyContent="Center">
                    
                <VBox justifyContent="Center" alignItems="Center" class="sapUiSmallMargin">
                    <Text text="Ir hacia la Vista 2" class="sapUiMediumMarginTop"></Text>
                    <Button text="Vista 2" press="onNavigateVista2" />
                </VBox>
                
                <VBox justifyContent="Center" alignItems="Center" class="sapUiSmallMargin">
                    <Text text="Ir hacia la Vista 3" class="sapUiMediumMarginTop"></Text>
                    <Button text="Vista 3" press="onNavigateVista3" />
                </VBox>
                

            </FlexBox>
        </content>
	</Page>
</mvc:View>
